{
  "name": "cyclone",
  "version": "1.0.0",
  "description": "An Apache Storm JavaScript implementation.",
  "main": "dist/index.js",
  "scripts": {
    "compile": "babel --optional runtime --compact false --out-dir dist index.js lib/*.js lib/**/*.js scripts/*.js",
    "test": "babel-node  --optional runtime test/harness.js test/*-test.js",
    "prepublish": "npm run compile",
    "generate": "babel-node scripts/generate.js"
  },
  "keywords": [
    "apache",
    "storm"
  ],
  "author": "Erik Toth <totherik@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel": "4.7.8",
    "espree": "^1.11.0",
    "glob": "^5.0.2",
    "tape": "^3.5.0"
  },
  "dependencies": {
    "babel-runtime": "4.7.8",
    "fs-extra": "^0.16.5",
    "thrift": "~0.9.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/totherik/cyclone.git"
  },
  "bugs": {
    "url": "https://github.com/totherik/cyclone/issues"
  },
  "homepage": "https://github.com/totherik/cyclone"
}
